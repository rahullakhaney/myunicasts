</div>
<%= link_to new_user_registration_path, class: 'button button--cta button--center button--top-spacing floating' do %>
  Sign up for free
<% end %>
</div>
<%= render 'layouts/footer' %>
</body>
<script>
    $(function(){
        $(".code-type").typed({
            strings: ["Ruby on Rails", "Git", "Nodejs", "Angularjs" ,"Reactjs", "Expressjs", "Docker"],
            typeSpeed: 0,
            loop: true,
            typeSpeed: 100,
            loopCount: 2,
        });
    });
</script>