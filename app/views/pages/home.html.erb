<body class="home" id="">
<%= render 'layouts/navbar' %>
<div class="jumbo">
<div class="row">
<div class="large-12 columns">
<% if user_signed_in? %>
<div class="jumbo__header">
Hi <%= current_user.first_name.capitalize %>.
</div>
<% else %>
<div class="jumbo__header">
Learn <span class="code-type"></span>
</div>
<div class="jumbo__subheader">
Learn to code and build something amazing with easy to follow tutorials
</div>
<% end %>


</div>
</div>
<%= link_to library_index_path, class: 'button button--cta button--center button--huge-spacing floating' do %>
  Browse the library&nbsp; <i class="fa fa-arrow-right"></i>
<% end %>
</div>
<div class="main-content">
<div class="centerHeader">
The latest from the library
</div>
<div class="row">
<div class="large-12 columns">
<div class="row">

<% @libraries.each do |library| %>
  <div class="large-4 columns end">
<div class="library-item library-item--fixed-height">
<div class="library-item__content">
<h2 class="library-item__title">
<a href="https://www.codecourse.com/library/lessons/laravel-events-and-listeners"><%= library.name %></a>
</h2>
<p><%= library.content %></p>
<div class="library-item__subjects">
<span class="tag">PHP</span>
<span class="tag">Laravel</span>
</div>
</div>
<footer class="library-item__footer clearfix">
<div class="list-inline">
<div class="list-inline__item">
<i class="fa fa-tasks"></i> <%= library.lessons.count %> parts
</div>
<div class="list-inline__item">
<i class="fa fa-clock-o"></i> <%= library.read_time %> minutes
</div>
<div class="list-inline__item">
<i class="fa fa-calendar-o"></i> <%= time_ago_in_words(library.created_at) %> ago
</div>
</div>
</footer>
</div>
</div>
<% end %>
</div>
</div>



<div class="row">
</div>
</div>
</div>
</div>
<div class="testimonials">
<div class="testimonials__header">
What people are saying
</div>
